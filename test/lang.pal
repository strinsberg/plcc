const int INT := 5;
const bool POINTLESS := true;
const float PI := 3.14;
const char SEMI := '$';



record Point
  int x, y;
endrec;

proc init_point (ref Point p; int a, b)
  p.x = a;
  p.y = b;
endproc;


record Point3D
  Point p;
  int z;
endrec;

proc init_point3D (ref Point3D p3; int a, b, c)
  p3.p.x = a;
  p3.p.y = b;
  p3.z = c;
endproc;


const int array ARR := [INT, 2, 3, 4, 5];
const bool vector VEC := [true, false, true];
const char -> int map MAP := {'a': 1, 'b': 2, 'c': 3};


#main
int a,b;
a, b = 1, 2;

Point p;
p::init_point (a, b);

Point p3;
p3::init_point3D (a, b, c);

call init_point(p, a, b);

#endmain
