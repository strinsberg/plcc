#includes
"my_lib.pal" as ml;
"some_lib.pal" as sl;


#constants
-- Basic type constants
-- In this section the const should be optional
const int INT = 5;
const bool POINTLESS = true;
const float PI = 3.14;
const char SEMI = ';';

-- Container constants
-- These expose that there could be no constants for custom types if types
-- are declared after constants, unless made in a module.
const int array ARR = [1, 2, 3, 4, 5];
const bool vector VEC = [true, false, true];
const char map int MAP = {'a': 1, 'b': 2, 'c': 3};


#types
Point
  int x, y;
end Point;

Point3D
  Point p;
  int z;
end Point3D;


#procedures
my_proc
  int x, y;
  write x;
end my_proc;

other_proc
  call my_proc;
end other_proc;


#main
int a, b;
call my_proc;
int array[10] A;
Point p;
write p;
