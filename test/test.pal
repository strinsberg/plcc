begin
  proc no_params
    write "no params", endl;
  end proc;

  proc copy (const int a, b)
    write a, b;
  end proc;

  proc refs (ref int b; ref int c)
    write b, c;
    b, c := 333, 555;
  end proc;

  const int c = 11111;
  int i, j;
  i, j := 1, 2;

  call no_params;

  call copy (i, c);
  write i, " ", j, endl;

  call refs (i, j);
  write i, " ", j, endl;
end.
