-- Finds the max and min elements in a list
begin
  const int n = 5;
  int array[n] A;
  int i, min, max;

  -- Input Table of n elements
  proc get_table
    i := 1;
    while not (i > n) do
      read A[i];
      i := i + 1;
    end while;
  end proc;

  -- Read table and set min and max to really high and low numbers
  call get_table;
  min, max := 1000000000, -1000000000;
  i := 1;

  -- Loop through A and find min and max
  while i < n + 1 do
    if A[i] < min then
        min := A[i];
    else if not (A[i] < min) then
        skip;
    end if;

    if A[i] > max then
        max := A[i];
    else if not (A[i] > max) then
        skip;
    end if;

    i := i + 1;
  end while;

  write min, max;
end.
